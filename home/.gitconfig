[user]
	name = Florian H.
	email = jscoderr@gmail.com
[push]
	default = current
[core]
	excludesfile = ~/.gitignore
[alias]
	auth = !ssh-add ~/id_rsa
	a = add
	aa = add -A
	b = branch
	c = commit -m
	ca = commit -am
	co = checkout
	cl = clone --reverse
	d = diff --word-diff=color
	f = fetch
	g = grep
	graph = log --oneline --graph
	h = help
	i = init
	ig = ignore
	k = !gitk
	l = log
	lo = log --oneline --graph --decorate
	m = merge
	p = push
	pt = !git push && git  push --tags
	pl = pull
	r = reset
	rb = rebase
	re = revert
	s = status
	sl = log --oneline
	ss = status -sb
	st = stash
	t = tag
	u = unstage
	unstage = reset HEAD
	uncommit = reset --soft HEAD^
	watch = !watch --interval=1 git status -s
	w = !git watch
[credential]
	helper = osxkeychain
[help]
	autocorrect = 7
[apply]
	whitespace = fix
[color]
	ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
