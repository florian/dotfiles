[user]
	name = Florian H.
	email = jscoderr@gmail.com
[push]
	default = current
[core]
	excludesfile = ~/.gitignore
[color]
	ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[alias]
	auth = !ssh-add ~/id_rsa
	unstage = reset HEAD
	uncommit = reset --soft HEAD^
	a = add
	aa = add -A
	b = branch
	c = commit -m
	ca = commit -am
	co = checkout
	d = diff
	f = fetch
	g = grep
	h = help
	i = init
	l = log
	m = merge
	p = push
	pt = !git push && git  push --tags
	pl = pull
	r = reset
	rb = rebase
	s = status
	ss = status -s
	st = stash
	t = tag
[credential]
	helper = osxkeychain
