fpath=(~/.zsh/Completion $fpath)

COMPLETION_WAITING_DOTS="true"
HISTSIZE=100000
TERM="screen-256color"

source "$HOME/.antigen/antigen.zsh"
antigen-lib

plugins=(bundler brew cake coffee command-not-found extract gem git git-extras heroku history-substring-search npm nyan osx pass redis-cli rvm thor zsh-syntax-highlighting)

antigen-theme apple
antigen-bundle zsh-users/zsh-syntax-highlighting
antigen-bundle zsh-users/zsh-completions src

unsetopt correct_all

PATH="$HOME/.bin:$PATH"
