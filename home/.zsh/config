fpath=(~/.zsh/Completion $fpath)

ZSH=$HOME/.oh-my-zsh
ZSH_THEME="apple"
COMPLETION_WAITING_DOTS="true"
HISTSIZE=100000

plugins=(bundler cake coffee command-not-found extract gem git git-extras heroku history-substring-search npm pass redis-cli rvm themes thor)

source $ZSH/oh-my-zsh.sh

unsetopt correct_all

PATH="$HOME/.bin:$PATH"